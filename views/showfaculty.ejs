<%- include("partials/nav", {user: user}) %>
<div class="row mt-5">
    <div class="col-md-6 m-auto">
        <h1 class="text-center mb-3">Faculty</h1>
    </div>
</div>
</div>
<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Age</th>
        <th scope="col">Gender</th>
        <th scope="col">Email</th>
        <th scope="col">Department</th>
    </tr>
    </thead>
    <tbody>
    <% for(let i=0; i<teacher.length; i++){ %>
    <tr>
        <td><%= teacher[i].t_id%></td>
        <td><a href="/teacher/course/<%= teacher[i].t_id%>"><%= teacher[i].t_name%></a></td>
        <td><%= teacher[i].t_age %></td>
        <td><%= teacher[i].t_gender%></td>
        <td><%= teacher[i].t_email %></td>
        <td><%= teacher[i].t_department%></td>
    </tr>
        <%}%>
    </tbody>
</table>
<%- include("partials/footer") %>